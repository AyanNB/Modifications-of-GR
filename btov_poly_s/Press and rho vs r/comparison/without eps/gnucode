1.5d16
set ytics 0.0,0.1,1.0

set tics font "Times Roman,14"
set key left bottom font "Times Roman,14"
set format y "%1.1f"
set multiplot


set object 1 rect from 7,0.5 to 13,0.9
set object 1 rect fc rgb 'white' fillstyle solid 0.0 noborder
plot "Mrb.dat" u 2:5 lt -1 dt 1 lw 1.3 title "1"  w l,\
"Mrb1" u 2:5 lt -1 dt (15,15,15,15) lw 1.3 title "2" w l,\
"Mrb1.5d16" u 2:5 lt 1 dt ". . . " lw 1.5 title "3" w l
set origin 0.42,0.46
set size 0.5,0.45
unset format y
set xrange [12:12.41]
set yrange [0:0.001]
set xtics 12,0.41,12.41
set xtics add("     12" 12 0)
set ytics 0,0.001,0.001
unset key
plot "Mrb.dat" u 2:5 lt -1 dt 1 lw 1.3 title "1"  w l,\
"Mrb1" u 2:5 lt -1 dt (15,15,15,15) lw 1.3 title "2" w l,\
"Mrb1.5d16" u 2:5 lt 1 dt ". . . " lw 1.5 title "3" w l


